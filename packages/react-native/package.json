{
  "name": "@vanikya/ota-react-native",
  "version": "0.2.5",
  "description": "OTA Update SDK for React Native apps - self-hosted CodePush/EAS Updates alternative",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "react-native.config.js",
    "app.plugin.js",
    "README.md"
  ],
  "scripts": {
    "build": "bob build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "bob build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-native",
    "ota",
    "update",
    "codepush",
    "eas-updates",
    "expo",
    "over-the-air",
    "hot-update"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanikya/ota-update.git",
    "directory": "packages/react-native"
  },
  "homepage": "https://vanikya.github.io/ota-update/",
  "bugs": {
    "url": "https://github.com/vanikya/ota-update/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-builder-bob": "^0.23.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.70.0",
    "expo-file-system": ">=15.0.0",
    "expo-crypto": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "expo-file-system": {
      "optional": true
    },
    "expo-crypto": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
